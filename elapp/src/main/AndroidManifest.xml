        <activity
            android:name=".activity.user.UserInfoActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activity.setting.BindUserAuthActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activity.HooViewHomeActivity" />
        </activity>
        <activity
            android:name=".activity.setting.BindPhoneActivity"
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activity.setting.BindUserAuthActivity" />
        </activity>
        <activity
            android:name=".activity.login.BindOrChangePhoneActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activity.setting.BindUserAuthActivity" />
        </activity>
        <activity
            android:name=".activity.setting.UpdateUserPasswordActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activity.setting.BindUserAuthActivity" />
        </activity>
        <activity
            android:name=".activity.user.FansListActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activity.user.FollowersListActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activity.HooViewHomeActivity" />
        </activity>
        <activity
            android:name=".activity.setting.AboutYishiyunActivity"
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activity.setting.AboutActivity" />
        </activity>
        <activity
            android:name=".activity.setting.CopyrightActivity"
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activity.setting.AboutActivity" />
        </activity>
        <activity
            android:name=".activity.setting.LiveMessageSetListActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activity.user.SettingActivity"
            android:screenOrientation="portrait"
